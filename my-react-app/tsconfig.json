{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": true,
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "baseUrl": "src", 
    "allowSyntheticDefaultImports": true, 
    "esModuleInterop": true
  },
  "include": ["src"]
}
